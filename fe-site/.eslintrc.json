{
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:jsx-a11y/recommended",
      "prettier"
      ],
    "plugins": [
        "react"
    ],
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "rules": {
        "react/jsx-uses-react": "off",
        "react/react-in-jsx-scope": "off"
      },
      "settings": {
        "react": {
          "version": "detect",
          "pragma": "React",
          "flowVersion": "0.53",
          "createClass": "createReactClass"
        }
      }
    
  }